{
  "name": "rescript-cats",
  "bsc-flags": ["-bs-super-errors", "-bs-no-version-header"],
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "tests",
      "type": "dev",
      "subdirs": true
    },
    {
      "dir": "examples",
      "type": "dev"
    }
  ],
  "suffix": ".mjs",
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "warnings": {
    "number": "+A",
    "error": "+A"
  },
  "gentypeconfig": {
    "language": "typescript",
    "shims": {},
    "generatedFileExtension": ".gen.ts",
    "module": "es6",
    "debug": {
      "all": false,
      "basic": false
    }
  },
  "bs-dev-dependencies": []
}
